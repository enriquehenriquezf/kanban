{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2020",
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react-native",
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@react-navigation/*": ["./node_modules/@react-navigation/*"]
    }
  }
}
